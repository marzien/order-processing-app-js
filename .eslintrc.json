{
    "extends": [
      "prettier",
      "prettier/standard",
      "prettier/react",
      "plugin:react/recommended"
    ],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error", {
            "endOfLine":"auto"
        }],
      "react/jsx-uses-react": "off",
      "react/react-in-jsx-scope": "off"
    },
    "parser": "babel-eslint",
    "env": {
      "browser": true
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    }
  }